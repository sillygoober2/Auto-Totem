{
	"schemaVersion": 1,
	"id": "auto-totem",
	"version": "${version}",
	"name": "Auto Totem",
	"description": "Auto Equip a totem after popping one! (OR Shield)",
	"authors": [
		"sillyygoober"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/auto-totem/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"github.sillygoober2.AutoTotem"
		],
		"client": [
			"github.sillygoober2.AutoTotemClient"
		]
	},
	"mixins": [
		"auto-totem.mixins.json",
		{
			"config": "auto-totem.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.5",
		"java": ">=21",
